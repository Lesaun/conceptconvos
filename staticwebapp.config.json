{
    "routes": [
      {
        "route": "/login",
        "rewrite": "/.auth/login/github"
      },
      {
        "route": "/logout",
        "redirect": "/.auth/logout"
      },
      {
        "route": "/*",
        "allowedRoles": ["administrator"]
      }
    ],
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "responseOverrides": {
      "401": {
        "redirect": "/login",
        "statusCode": 302
      }
    },
    "mimeTypes": {
      ".json": "text/json"
    }
}